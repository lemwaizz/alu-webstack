defaults
  mode http
  timeout client 15s
  timeout connect 10s
  timeout server 15s
  timeout http-request 10s

frontend mwai1-tech-frontend
    bind *:443 ssl crt /etc/letsencrypt/renewal/www.mwai1.tech.conf/fullchain.pem
    default_backend mwai1-tech-backend

backend mwai-tech-backend
    balance roundrobin
    server 5987-web-02 54.196.153.154:80 check
    server 5987-web-01 54.227.116.16:80 check
